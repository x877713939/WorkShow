{% if type in ['essay', 'choice', 'single_choice', 'uncertain_choice'] %}
  <div class="row cd-mv16">
    <div class="col-sm-2 control-label subject-change-lable">{{ 'course.testpaper_manage.import.re_edit.convert_hint'|trans }}ï¼š</div>
    <div class="col-sm-9 subject-change-list" data-convert-url="{{ path('subject_convert_template') }}" data-from-type="{{ type }}">
    {% if type == 'essay' %}
      <button class="cd-btn cd-btn-ghost-primary subject-change-btn" type="button" data-type="single_choice">{{ 'question.type.single_choice'|trans }}</button>
      <button class="cd-btn cd-btn-ghost-primary subject-change-btn" type="button" data-type="uncertain_choice">{{ 'question.type.uncertain_choice'|trans }}</button>
      <button class="cd-btn cd-btn-ghost-primary subject-change-btn" type="button" data-type="choice">{{ 'question.type.multiple_choice'|trans }}</button>
      <button class="cd-btn cd-btn-ghost-primary subject-change-btn" type="button" data-type="fill">{{ 'question.type.fill'|trans }}</button>
      <button class="cd-btn cd-btn-ghost-primary subject-change-btn" type="button" data-type="determine">{{ 'question.type.determine'|trans }}</button>
    {% elseif type == 'choice' %}
      <button class="cd-btn cd-btn-ghost-primary subject-change-btn" type="button" data-type="single_choice">{{ 'question.type.single_choice'|trans }}</button>
      <button class="cd-btn cd-btn-ghost-primary subject-change-btn" type="button" data-type="uncertain_choice">{{ 'question.type.uncertain_choice'|trans }}</button>
    {% elseif type == 'single_choice' %}
      <button class="cd-btn cd-btn-ghost-primary subject-change-btn" type="button" data-type="choice">{{ 'question.type.multiple_choice'|trans }}</button>
      <button class="cd-btn cd-btn-ghost-primary subject-change-btn" type="button" data-type="uncertain_choice">{{ 'question.type.uncertain_choice'|trans }}</button>
    {% elseif type == 'uncertain_choice' %}
      <button class="cd-btn cd-btn-ghost-primary subject-change-btn" type="button" data-type="single_choice">{{ 'question.type.single_choice'|trans }}</button>
      <button class="cd-btn cd-btn-ghost-primary subject-change-btn" type="button" data-type="choice">{{ 'question.type.multiple_choice'|trans }}</button>
    {% endif %}
    </div>
  </div>
{% endif %}