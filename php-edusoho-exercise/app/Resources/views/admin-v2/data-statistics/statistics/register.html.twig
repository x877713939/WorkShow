{% extends 'admin-v2/data-statistics/statistics/analysis-base-layout.html.twig' %}
{% set script_controller = 'analysis/register' %}
{% block analysisBody %}
<div class="col-md-12">
  {% if tab=="trend" %}
	 <div id="line-data">
   </div>
   <input id="data"  type="hidden" value='{% if data %}{{data}}{% endif %}'>
   <input id="registerStartDate"  type="hidden" value='{% if registerStartDate %}{{registerStartDate}}{% endif %}'>
  {% elseif tab=="detail" %}
    {% include 'admin-v2/data-statistics/statistics/register.table.html.twig' %}
  {% endif %}
</div>
{% endblock %}

